    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.15.1",
    "axios": "^1.2.0",
    "bootstrap": "^5.2.3",
    "bootstrap-vue": "^2.23.1",
    "vue": "^2.7.14",
    "vue-router": "^3.5.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "vite": "^3.2.3"
  }
}
